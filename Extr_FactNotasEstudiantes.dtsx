                    <property
                      dataType="System.String"
                      description="Comando SQL para ejecutar."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
C.cod_curso,
P.identificacion_Profesor,
E.identificacion_Estudiante,
NE.nota,
NE.fecha_desde,
NE.fecha_hasta,
NE.fecha_registro
FROM NOTAS_ESTUDIANTES NE 
LEFT JOIN ESTUDIANTE E ON
E.pk_Estudiante=NE.pk_estudiante
LEFT JOIN CURSO C ON
C.pk_curso= NE.pk_curso
LEFT JOIN PROFESOR P ON
P.pk_profesor=C.pk_curso
WHERE CAST(CONVERT(VARCHAR,NE.fecha_registro,112) AS INT) BETWEEN ? AND ?</property>
